{
    "pyvision": {
        "project_name": "hello_world",
        "entry_point": "../model/hello.py",
        "plotter": "../model/plot.py",
        "required_packages": [
            // source files which should be tracked
            "../model"
        ],
        "optional_packages": [
            // source files which may be tracked
            "../../pyvision",
            "../../mutils"
        ],
        // Copy packages to logdir?
        "copy_required_packages": true,
        "copy_optional_packages": true
    },
    "plotting": {
        "defaults": {
            "plot": [
                "Performance",
                "PerformanceSquare"
            ],
            "prefixes": [
                "Val",
                "Val"
            ],
            "titles": [
                "Performance",
                "Performance Squared"
            ]
        },
        "num_smoothing_samples": 8
    },
    "training": {
        "num_epochs": 12,
        "steps_per_epoch": 5,
        "default_device": "cpu"
    },
    "model": {
        "keyword": "Hello, World!"
    }
}